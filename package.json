{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with VanillaJS and NodeJS",
  "author": "Ingkells",
  "dependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.9",
    "@babel/node": "^7.12.1",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "autoprefixer": "^10.0.1",
    "aws-sdk": "^2.787.0",
    "axios": "^0.21.0",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-flash": "0.0.2",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.1.1",
    "mini-css-extract-plugin": "^1.2.1",
    "mongoose": "^5.10.10",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node": "^15.0.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^4.0.4",
    "pug": "^3.0.0",
    "sass": "^1.28.0",
    "sass-loader": "^7.1.0",
    "shx": "^0.3.3",
    "sudo": "^1.0.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^4.1.0"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && cross-env WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && cross-env WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "shx cp -r src/static build && shx cp -r src/views build",
    "tunnel": "lt --port 4000",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "start": "node build/init.js",
    "prestart": "npm run build"
  },
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.6",
    "prettier": "2.1.2"
  }
}
